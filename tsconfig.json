{
  "compilerOptions": {
    "lib": [
      "es2017",
      "dom"
    ],
    "removeComments": true,
    "moduleResolution": "node",
    "sourceMap": true,
    "module": "commonjs",
    "target": "es5",
    "jsx": "react",
    "esModuleInterop": true
  },
  "include": [
    "src/**/*",
  ],
  "exclude": [
    "node_modules/**/*"
  ]
}